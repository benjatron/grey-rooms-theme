{
  "name": "fwd-boilerplate-theme",
  "version": "2.0",
  "description": "WordPress boilerplate theme for use as a starting point for development",
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "watch": "webpack --watch --config config/webpack.config.js"
  },
  "repository": "https://github.com/designfwd/fwd-boilerplate-theme.git",
  "author": "FWD Creative LLC",
  "license": "GPL-3.0",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 1,
      "no-undef": 1,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "cookieconsent": "^3.1.1",
    "dialog-polyfill": "^0.5.0",
    "instant.page": "^3.0.0",
    "lazysizes": "^5.2.0-beta1",
    "mmenu-js": "^8.4.3",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bourbon": "^6.0.0",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "file-loader": "^5.0.2",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.13.0",
    "postcss-dialog-polyfill": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  }
}
