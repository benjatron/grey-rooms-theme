{
  "name": "fwd-boilerplate-theme",
  "version": "2.0",
  "description": "WordPress boilerplate theme for use as a starting point for development",
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "watch": "webpack --watch --config config/webpack.config.js"
  },
  "repository": "https://github.com/designfwd/fwd-boilerplate-theme.git",
  "author": "FWD Creative LLC",
  "license": "GPL-3.0",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 1,
      "no-undef": 1,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "cookieconsent": "^3.1.1",
    "dialog-polyfill": "^0.5.0",
    "instant.page": "^3.0.0",
    "lazysizes": "^5.2.0",
    "mmenu-js": "^8.5.3",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bourbon": "^7.0.0",
    "css-loader": "^3.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-dialog-polyfill": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.3.1",
    "stylelint-scss": "^3.16.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
