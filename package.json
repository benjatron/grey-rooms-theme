{
  "name": "fwd-boilerplate-theme",
  "version": "1.1.0",
  "description": "WordPress boilerplate theme for use as a starting point for development",
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "watch": "webpack --watch --config config/webpack.config.js"
  },
  "repository": "https://github.com/designfwd/fwd-boilerplate-theme.git",
  "author": "FWD Creative LLC",
  "license": "GPL-3.0",
  "browserslist": [
    "> 1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
      "no-console": 1,
      "no-undef": 1,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "dialog-polyfill": "^0.5.0",
    "lazysizes": "^5.1.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "bourbon": "^5.1.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "file-loader": "^4.0.0",
    "image-webpack-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "node-sass-magic-importer": "^5.3.2",
    "postcss-custom-properties": "^9.0.1",
    "postcss-dialog-polyfill": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-scss": "^3.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
